/**Crie um algoritmo que leia dois números inteiros e mostre a diferença absoluta entre eles (sem sinal negativo). */
programa {
  inclua biblioteca Matematica-->mat
  funcao inicio() {
  inteiro numero1,numero2,diferenca
  escreva("informe um número inteiro: ")  
  leia(numero1)
  escreva("informe um número inteiro: ")  
  leia(numero2)
  diferenca=numero1-numero2
  escreva("diferença absoluta é: ",diferenca)
  escreva("A diferenca absoluta é: ",m.valor_absoluto(diferenca))

  }
}
